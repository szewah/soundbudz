{
    "name": "soundbudz",
    "version": "0.0.0",
    "description": "A fullstack app that builds friendships around music events",
    "keywords": [
        "express",
        "api",
        "groups",
        "react",
        "ticketmaster"
    ],
    "main": "app.js",
    "private": true,
    "scripts": {
        "start": "if-env NODE_ENV-production && npm start:prod || npm run start:dev",
        "start:prod": "node app.js",
        "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
        "client": "cd client && npm run start",
        "seed": "node scripts/seedDB.js",
        "install": "cd client && npm install",
        "build": "cd client && npm run build",
        "heroku-postbuild": "npm run build"
    },
    "dependencies": {
        "axios": "^0.21.1",
        "bcryptjs": "^2.4.3",
        "body-parser": "^1.19.0",
        "concurrently": "^5.0.0",
        "cookie-parser": "~1.4.4",
        "cors": "^2.8.5",
        "debug": "~4.1.1",
        "dotenv": "^8.1.0",
        "express": "~4.17.1",
        "helmet": "^3.21.1",
        "http-errors": "~1.7.3",
        "if-env": "^1.0.4",
        "jquery": "^3.4.1",
        "jsonwebtoken": "^8.5.1",
        "jwt-decode": "^2.2.0",
        "moment": "^2.24.0",
        "morgan": "~1.9.1",
        "mysql2": "^1.7.0",
        "passport": "^0.4.0",
        "passport-jwt": "^4.0.0",
        "sequelize": "^5.19.4",
        "sequelize-cli": "^5.5.1",
        "socket.io": "^2.3.0"
    }
}
